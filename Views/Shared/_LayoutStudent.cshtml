@model AP_Project.Models.Users.Student
@{
  Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}

@section Styles {
  <link rel="stylesheet" href="~/css/Layout/_layout-dashboard.css"  asp-append-version="true" />
  <link rel="stylesheet" href="~/css/Layout/_layout-student.css" asp-append-version="true" />
}

@section Header {
  @await Html.PartialAsync("~/Views/Shared/Header/_StudentHeaderPartial.cshtml", Model)
}

@RenderBody()
@RenderSection("Scripts", required: false)

@section Popup{ <!-- انتقال پاپ آپ به لایه بالاتر -->
  @RenderSection("Popup", required: false)
}